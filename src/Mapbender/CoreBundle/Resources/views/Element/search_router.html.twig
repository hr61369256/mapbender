<div class="mb-element mb-element-searchrouter" id="{{ element.id }}" title="{{ element.title | trans }}">
    {{ form_label(element.getRouteSelectForm.route) }}{{ form_widget(element.getRouteSelectForm.route) }}

    <div class="search-forms" novalidate="novalidate">
        {% for form in element.forms %}
            <form name="{{ form.getName() }}">
                {{ form_widget(form.createView) }}
            </form>
        {% endfor %}
    </div>
    <div class="clear"></div>
    <div class="search-results"></div>
</div>
